{
  "name": "@entur/sdk",
  "version": "1.6.0",
  "license": "EUPL-1.2",
  "main": "./lib/index.js",
  "repository": "github:entur/sdk",
  "bugs": {
    "url": "https://github.com/entur/sdk/issues"
  },
  "browser": {
    "./lib/fetch.js": "./lib/fetch.browser.js"
  },
  "typings": "./index.d.ts",
  "files": [
    "lib",
    "index.d.ts",
    "libdef.flow.js"
  ],
  "scripts": {
    "lint": "eslint src/ types/ scripts/ --ext=js,ts",
    "test": "npm run lint && npm run check-libdefs && npm run validate-queries && tsc --noEmit -p .",
    "validate-queries": "ts-node scripts/validate-queries.js",
    "check-libdefs": "flow && tsc index.d.ts --noEmit",
    "prebuild": "npm test && rm -rf lib",
    "build": "babel --extensions .ts src --out-dir lib",
    "prepublishOnly": "npm run build",
    "docs": "docz dev",
    "build:docs": "docz build",
    "deploy:docs": "npm run build:docs && firebase deploy",
    "fetch-schemas": "ts-node scripts/fetch-schemas.ts"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.1",
    "@turf/bbox": "^6.0.1",
    "@turf/destination": "^6.0.1",
    "@turf/helpers": "^6.1.4",
    "clean-deep": "^3.3.0",
    "node-fetch": "^2.6.0",
    "promise-throttle": "^1.1.0",
    "qs": "^6.9.4",
    "regenerator-runtime": "^0.13.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.1",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/register": "^7.10.1",
    "@types/node-fetch": "^2.5.7",
    "@types/qs": "^6.9.3",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-eslint": "^10.1.0",
    "docz": "^2.3.1",
    "eslint": "^6.8.0",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "firebase-tools": "^7.16.2",
    "flow-bin": "^0.89.0",
    "gatsby-theme-docz": "^2.3.1",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  }
}
